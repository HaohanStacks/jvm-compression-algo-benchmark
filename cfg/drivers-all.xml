<?xml version="1.0" encoding="UTF-8"?>

<!-- Definition of all JVM compressor codecs
  -->

<driverGroup name="javaDrivers"
  xmlns="http://www.sun.com/japex/testSuite"
  xmlns:xi="http://www.w3.org/2001/XInclude"
>
  <!--  Let's use LZF as the normalized value for now -->
  <driver name="LZF/block" normal="true">
        <description><div xmlns=""><p>Test driver for LZF, block mode
</p></div></description>
        <param name="japex.classPath" value="build/classes"/>
        <param name="japex.classPath" value="lib/*.jar"/>
        <param name="japex.classPath" value="lib/lzf/*.jar"/>
        <param name="japex.driverClass" value="com.ning.jcbm.lzf.LzfDriver" />
  </driver>

  <driver name="QuickLZ/level-1/block">
        <description><div xmlns=""><p>Test driver for QuickLZ (level 1), block mode
</p></div></description>
        <param name="japex.classPath" value="build/classes"/>
        <param name="japex.classPath" value="lib/*.jar"/>
        <param name="japex.driverClass" value="com.ning.jcbm.quicklz.QuickLZDriver1" />
  </driver>

<!--  
  <driver name="QuickLZ/level-3/block">
        <description><div xmlns=""><p>Test driver for QuickLZ (level 3), block mode
</p></div></description>
        <param name="japex.classPath" value="build/classes"/>
        <param name="japex.classPath" value="lib/*.jar"/>
        <param name="japex.driverClass" value="com.ning.jcbm.quicklz.QuickLZDriver3" />
  </driver>
-->

</driverGroup>
