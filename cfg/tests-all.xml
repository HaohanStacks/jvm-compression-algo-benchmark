<?xml version="1.0" encoding="UTF-8"?>

<testSuite name="Test suite for JVM compressors"
  xmlns="http://www.sun.com/japex/testSuite"
  xmlns:xi="http://www.w3.org/2001/XInclude"
>
    <description><div xmlns=""><p>
Micro-benchmark for testing performance of various xml-parser/xsl-processor
combinations.
</p></div></description>

    <!-- Drivers -->
    <xi:include href="drivers-all.xml" />

    <!-- Test cases: name has to match with filename -->

<testCase name='build.xml'><param name='japex.operation' value='COMPRESS' /></testCase>
<testCase name='lzf.jar'><param name='japex.operation' value='COMPRESS' /></testCase>

<testCase name='build.xml'><param name='japex.operation' value='UNCOMPRESS' /></testCase>
<testCase name='lzf.jar'><param name='japex.operation' value='UNCOMPRESS' /></testCase>

</testSuite>
